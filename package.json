{
  "name": "wp-export-import",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "clean:src": "find src -name '*.js' -o -name '*.js.map' -o -name '*.d.ts.map' | xargs rm -f 2>/dev/null || true && find src -name '*.d.ts' ! -path 'src/utils/types/*' | xargs rm -f 2>/dev/null || true",
    "compile": "tsc --project tsconfig.build.json",
    "build": "npm run clean && npm run clean:src && tsc",
    "lint": "eslint src --ext .ts,.js",
    "type-check": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "tsx watch src/index.ts",
    "start": "node -r tsconfig-paths/register dist/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.14.0",
  "dependencies": {
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "multer": "^2.0.2",
    "pino": "^9.9.0",
    "tsconfig-paths": "^4.2.0",
    "zod": "^4.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.34.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0"
  }
}
